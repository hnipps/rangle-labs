{
  "name": "rangle-labs",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "start": "lerna run start --progress --stream --parallel",
    "seed": "lerna run seeds --progress --stream",
    "migrate": "lerna run migrate:down --progress --stream",
    "migrate:up": "lerna run migrate:up --progress --stream",
    "build": "lerna run build --progress --stream",
    "prep": "lerna run prep --progress --stream",
    "run:watch": "lerna run --parallel run:watch",
    "fix": "lerna run fix",
    "test": "lerna run test",
    "setup": "lerna run setup && lerna bootstrap"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "jest": "^23.5.0",
    "lerna": "^3.3.0",
    "prettier-cli": "^0.1.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.1.3",
    "ts-loader": "^5.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  }
}
